{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest -watch",
    "eslint": "eslint --ext .js ./",
    "eslint:fix": "eslint --fix --ext .js ./",
    "prepush": "npm run eslint && npm test"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run eslint && npm test"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statments": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "keywords": [
    "ropenflash"
  ],
  "author": "rishab gupta",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^25.5.4"
  }
}